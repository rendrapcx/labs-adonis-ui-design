<aside x-cloak x-data="{active: '{{active}}' }">
  <div
    class="flex flex-col w-64 h-screen overscroll-y-auto py-8 bg-white border-r dark:bg-gray-900 dark:border-gray-700">
    {{--  BRAND  --}}
    <h2 class="text-3xl font-semibold text-center text-gray-800 dark:text-white">-:{ RPCX }:-</h2>

    {{--  USER FOTO NAME EMAIL  --}}
    <div class="flex flex-col items-center mt-6 -mx-2">
      <img class="object-cover w-24 h-24 mx-2 rounded-full"
        {{--  src="https://images.unsplash.com/photo-1531427186611-ecfd6d936c79?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=634&q=80"  --}}
        src="https://i.imgur.com/4Qa801d.jpg" alt="avatar">
      <h4 class="mx-2 mt-2 font-medium text-gray-800 dark:text-gray-200 hover:underline cursor-pointer">{{sbName}}</h4>
      <p class="mx-2 mt-1 text-sm font-medium text-gray-600 dark:text-gray-400 hover:underline cursor-pointer">
        {{sbEmail}}</p>
      <p>
    </div>

    {{--  SIDEBAR MENU  --}}
    <div class="flex flex-col justify-between flex-1 mt-6">
      <nav>
        {{--  DASHBOARD  --}}
        <div>
          <a class="flex items-center px-4 py-2 mt-5 text-gray-600 transition-colors duration-300 transform
          dark:text-gray-400 hover:bg-gray-200 dark:hover:bg-gray-700 dark:hover:text-gray-200 hover:text-gray-700"
            :class="active == 'dashboard' ? 'bg-gray-100 dark:bg-gray-800 dark:text-gray-200' : ''"
            href="{{route('cs.dashboard')}}">
            <svg class="w-5 h-5" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path
                d="M19 11H5M19 11C20.1046 11 21 11.8954 21 13V19C21 20.1046 20.1046 21 19 21H5C3.89543 21 3 20.1046 3 19V13C3 11.8954 3.89543 11 5 11M19 11V9C19 7.89543 18.1046 7 17 7M5 11V9C5 7.89543 5.89543 7 7 7M7 7V5C7 3.89543 7.89543 3 9 3H15C16.1046 3 17 3.89543 17 5V7M7 7H17"
                stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
            </svg>
            <span class="mx-4 font-medium">Dashboard</span>
          </a>
        </div>

        {{--  ACCOUNTS  --}}
        <div>
          <a class=" flex items-center px-4 py-2 mt-5 text-gray-600 transition-colors duration-300 transform
            dark:text-gray-400 hover:bg-gray-200 dark:hover:bg-gray-700 dark:hover:text-gray-200 hover:text-gray-700"
            :class="active == 'account' ? 'bg-gray-100 dark:bg-gray-800 dark:text-gray-200' : ''"
            href="{{route('cs.profile')}}">
            <svg class="w-5 h-5" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path
                d="M16 7C16 9.20914 14.2091 11 12 11C9.79086 11 8 9.20914 8 7C8 4.79086 9.79086 3 12 3C14.2091 3 16 4.79086 16 7Z"
